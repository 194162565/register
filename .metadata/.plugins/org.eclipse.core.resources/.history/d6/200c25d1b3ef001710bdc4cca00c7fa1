package com.sxt.copy;

import java.io.File;

public class Test {
	public static void main(String[] args) throws Exception {
		String filepath = "D:/IO/test";
		File file = new File(filepath);
		if(!file.exists()){
			file.createNewFile();
		}else{
			System.out.println("无法创建");
		}
	
	//	System.out.println(file.delete());
	


	}
}
